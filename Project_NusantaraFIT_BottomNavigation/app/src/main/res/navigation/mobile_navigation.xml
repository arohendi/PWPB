<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.project_nusantarafit.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/home_Fragment" />

    <fragment
        android:id="@+id/navigation_quiz"
        android:name="com.example.project_nusantarafit.QuizFragment"
        android:label="@string/title_quiz"
        tools:layout="@layout/quiz_Fragment"/>

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.example.project_nusantarafit.ProfileFragment"
        android:label="@string/title_profile"
        tools:layout="@layout/Profile_Fragment" />

    <fragment
        android:id="@+id/navigation_camera"
        android:name="com.example.project_nusantarafit.CameraFragment"
        android:label="@string/title_camera"
        tools:layout="@layout/camera_fragment" />

</navigation>