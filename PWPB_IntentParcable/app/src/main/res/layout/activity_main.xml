<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    android:orientation="vertical"
    android:layout_marginBottom="@dimen/activit_vertical_margin"
    android:layout_marginTop="@dimen/activit_vertical_margin"
    android:layout_marginLeft="@dimen/activit_horizontal_margin"
    android:layout_marginRight="@dimen/activit_horizontal_margin">

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/btn_move_activity"
        android:text="pindah activity"
        android:layout_marginBottom="@dimen/activit_vertical_margin"/>
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/btn_move_with_data_activity"
        android:text="pindah activity dengan data"
        android:layout_marginBottom="@dimen/activit_vertical_margin"/>
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/btn_move_activity_object"
        android:text="pindah activity dengan object"
        android:layout_marginBottom="@dimen/activit_vertical_margin"/>
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/btn_dial_number"
        android:text="dial_number"
        android:layout_marginBottom="@dimen/activit_vertical_margin"/>
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/btn_move_for_result"
        android:text="move_with_result"
        android:layout_marginBottom="@dimen/activit_vertical_margin"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/tv_result"
        android:gravity="center"
        android:text="result_from_activity"
        android:textSize="24sp"/>
</LinearLayout>